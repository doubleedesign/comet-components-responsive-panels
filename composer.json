{
    "name": "doubleedesign/comet-responsive-panels",
    "description": "Standalone package for ResponsivePanels from the Comet Components library",
    "version": "0.0.2",
    "homepage": "https://cometcomponents.io",
    "authors": [
        {
            "name": "Leesa Ward",
            "email": "leesa@doubleedesign.com.au"
        }
    ],
    "autoload": {
        "classmap": [
            "dist/src/components/",
            "dist/src/components/**/"
        ],
        "psr-4": {
            "Doubleedesign\\Comet\\Core\\": [
                "dist/src/base/types/",
                "dist/src/base/types/",
                "dist/src/base/attributes/",
                "dist/src/base/traits/",
                "dist/src/base/components/",
                "dist/src/base/",
                "dist/src/services/"
            ]
        }
    },
    "require": {
        "doubleedesign/comet-standalone-launchpad": "0.0.2"
    }
}